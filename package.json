{
  "name": "poker_tracker",
  "version": "1.0.0",
  "description": "Poker tracker built with MERN stack and redux",
  "main": "server.js",
  "repository": "github:Ponta1337/poker_tracker",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Pontus",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bootstrap-icons": "^1.2.0",
    "chart.js": "^2.9.4",
    "concurrently": "^5.3.0",
    "config": "^3.3.2",
    "dateformat": "^4.0.0",
    "express": "^4.17.1",
    "heroku": "^7.47.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongoose": "^5.12.2",
    "ordinal": "^1.0.3",
    "react": "^17.0.1",
    "react-bootstrap-tabs": "^2.0.0",
    "react-chartjs-2": "^2.11.1",
    "react-datepicker": "^3.3.0",
    "react-dom": "^17.0.1",
    "react-infinite-scroll-component": "^6.0.0",
    "react-media-hook": "^0.4.9",
    "react-number-format": "^4.5.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
